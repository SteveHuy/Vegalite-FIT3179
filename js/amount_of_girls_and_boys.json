{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 300,
  "config": {
    "background": "#C9EDDC" 
  },
  "data": {
    "url": "https://raw.githubusercontent.com/SteveHuy/Vegalite-FIT3179/main/data/Minimised%20Powerlifting%20Data.csv"
  },
  "transform": [
    {
      "joinaggregate": [{"op": "count", "as": "count"}],
      "groupby": ["Age", "Tested"]
    }
  ],
  "layer": [
    {
      "mark": "area",
      "encoding": {
        "x": {"field": "Age", "title": "Age", "type": "quantitative"},
        "y": {"aggregate": "count", "type": "quantitative", "title": "Count"},
        "color": {"field": "Tested", "scale": {"scheme": "category10"}},
        "tooltip": [
          {"field": "Age", "type": "quantitative", "title": "Age"},
          {"field": "Tested", "type": "nominal", "title": "Sex"},
          {"field": "count", "type": "quantitative", "title": "Count"}
        ]
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "right",
        "baseline": "top",
        "dx": 240,
        "dy": -140,
        "fontSize": 10,
        "fontStyle": "italic"
      }
      
    }
  ]
}