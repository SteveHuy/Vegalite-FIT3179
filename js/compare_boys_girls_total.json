{
    "width": 500,
    "height": 300,
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/SteveHuy/Vegalite-FIT3179/main/data/Minimised%20Powerlifting%20Data.csv"
    },
    "mark": {"type": "line", "point": true},
    "encoding": {
      "x": {"bin": true, "field": "BodyweightKg", "type": "ordinal", "title":"Body Weight in Kg"},
      "y": {"aggregate": "average", "field": "TotalKg", "type": "quantitative"},
      "color": {"field": "Sex", "type": "nominal"},
      "tooltip": [
        {"field": "bin_maxbins_10_BodyweightKg_range", "title": "Bodyweight (Bin)"},
        {"field": "average_TotalKg", "title": "Average TotalKg"},
        {"field": "Sex", "title": "Sex"}
      ]
    }
  }
  